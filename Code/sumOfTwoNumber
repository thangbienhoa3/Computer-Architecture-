.data 
	prompt1: .asciz "Enter value of the first number: "
	prompt2: .asciz "Enter value of the second number: "
	result: .asciz "The sum is: "
.text 
	la a0,prompt1
	addi a7,zero,4
	ecall
	li a7,5
	ecall
	addi t0,a0,0
	
	la a0,prompt2
	addi a7,zero,4
	ecall
	li a7,5
	ecall
	addi t1,a0,0
	
	add t2,t0,t1
	
	la a0,result
	addi a7,zero,4
	ecall 
	
	addi a0,t2,0
	addi a7,zero,1
	ecall 
	
	addi a7,zero,10 #exit program
	ecall
	